    "dev": "nodemon -r dotenv/config .",
    "lint": "eslint .",
    "format": "npx prettier --write \"**/*.js\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint-staged": "lint-staged",
    "prepare": "node .husky/install.js",
    "migrate:make": "npx knex --knexfile db/knexfile.js --env=dev migrate:make",
    "migrate:latest": "npx knex --knexfile db/knexfile.js --env=dev migrate:latest",
    "migrate:down": "npx knex --knexfile db/knexfile.js --env=dev migrate:down",
    "seed:make": "npx knex --knexfile db/knexfile.js --env=dev seed:make",
    "seed:run": "npx knex --knexfile db/knexfile.js --env=dev seed:run"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --cache-strategy content --fix"
    ],
    "*.{json,js,html}": [
      "prettier --write --ignore-unknown"
    ]
  },
